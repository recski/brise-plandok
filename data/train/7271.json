s": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"VerkehrsflaecheID": {"name": "VerkehrsflaecheID", "value": null, "type": null}}, "annotated_attributes": {"DON'T ANNOTATE THIS SENTENCE": {"annotators": ["01", "04"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {"DON'T ANNOTATE THIS SENTENCE": {"value": [], "type": null, "name": "DON'T ANNOTATE THIS SENTENCE"}}, "full_gold_exists": true, "full_annotated_attributes": {}, "gold_modality": null, "predicted_attributes": {"VerkehrsflaecheID": {"value": ["Siebenb\u00fcrgerstra\u00dfe", "Erzherzog-Karl-Stra\u00dfe"], "type": "condition", "name": "VerkehrsflaecheID"}}}, "7271_1_0": {"id": "7271_1_0", "text": "Bezirk, Kat. G. Kagran und Stadlau werden unter Anwendung des \u00a7 1 der BO f\u00fcr Wien folgende Bestimmungen getroffen:", "modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": true, "full_annotated_attributes": {}, "gold_modality": null, "predicted_attributes": {}}, "7271_2_0": {"id": "7271_2_0", "text": "Der bisher g\u00fcltige Fl\u00e4chenwidmungsplan und Bebauungsplan verliert seine weitere Rechtskraft.", "modality": null, "already_gold_on_annotation": false, "labels_gold_exists": true, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {"PlangebietAllgemein": {"annotators": ["04"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": true, "full_annotated_attributes": {}, "gold_modality": null, "predicted_attributes": {}}, "7271_4_0": {"id": "7271_4_0", "text": "Die roten Planzeichen gelten als neu festgesetzt.", "modality": null, "already_gold_on_annotation": true, "labels_gold_exists": true, "gold_attributes": {}, "gen_attributes_