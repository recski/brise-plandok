ion": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"GebaeudeHoeheMax": {"name": "GebaeudeHoeheMax", "value": null, "type": null}}, "annotated_attributes": {"GebaeudeHoeheMax": {"annotators": ["06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7389_13_0": {"id": "7389_13_0", "text": "Auf den mit BB4 bezeichneten Fl\u00e4chen ist die Errichtung von Geb\u00e4uden f\u00fcr die erholungssuchende Bev\u00f6lkerung mit einer maximalen Geb\u00e4udeh\u00f6he von 4 m zul\u00e4ssig.", "modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"GebaeudeHoeheMax": {"name": "GebaeudeHoeheMax", "value": null, "type": null}, "Planzeichen": {"name": "Planzeichen", "value": null, "type": null}}, "annotated_attributes": {"GebaeudeHoeheMax": {"annotators": ["06"]}, "Planzeichen": {"annotators": ["06"]}, "WidmungUndZweckbestimmung": {"annotators": ["06"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7389_14_0": {"id": "7389_14_0", "text": "Es wird darauf hingewiesen, dass bei Errichtung von Kellern auf den schwankenden Grundwasserspiegel zu achten ist.", "modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7389_14_1": {"id": "7389_14_1", "text": "Der Abteilungsleiter: Dipl.-Ing. Binder Senatsrat", "modality": null, "already_gold_on_annotation": true, "labels_gold_exists": true, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": 