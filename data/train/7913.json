_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"Planzeichen": {"name": "Planzeichen", "value": null, "type": null}}, "annotated_attributes": {"Flaechen": {"annotators": ["04"]}, "Kleinhaeuser": {"annotators": ["04"]}, "Planzeichen": {"annotators": ["04"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7913_21_1": {"id": "7913_21_1", "text": "Die Trakttiefe der Geb\u00e4ude wird mit 15,0 m beschr\u00e4nkt.", "modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {"GebaeudeHoeheMax": {"annotators": ["04"]}, "HoehenlageGrundflaeche": {"annotators": ["04"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7913_21_2": {"id": "7913_21_2", "text": "Der Abteilungsleiter: Dipl.-Ing. Walter Krauss Senatsrat", "modality": null, "already_gold_on_annotation": true, "labels_gold_exists": true, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}}, "annotators": ["04"], "labels_gold": false, "full_gold": false}