l}}, "annotated_attributes": {"Planzeichen": {"annotators": ["03"]}, "WidmungInMehrerenEbenen": {"annotators": ["03"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7973_25_0": {"id": "7973_25_0", "text": "F\u00fcr die mit BB7 bezeichneten Grundfl\u00e4chen wird bestimmt: Der Raum unterhalb einer 10,0 m \u00fcber dem Niveau der angrenzenden Verkehrsfl\u00e4che liegenden Ebene ist der \u00f6ffentlichen Verkehrsfl\u00e4che zugeordnet, der Raum dar\u00fcber der Widmung Bauland/Gemischtes Baugebiet - Gesch\u00e4ftsviertel mit den im Plan angegebenen Bestimmungen.", "modality": null, "already_gold_on_annotation": false, "labels_gold_exists": false, "gold_attributes": {}, "gen_attributes_on_annotation": {"Planzeichen": {"name": "Planzeichen", "value": null, "type": null}}, "annotated_attributes": {"Planzeichen": {"annotators": ["03"]}, "WidmungInMehrerenEbenen": {"annotators": ["03"]}}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}, "7973_25_1": {"id": "7973_25_1", "text": "Der Abteilungsleiter: Dipl.-Ing. Krauss Senatsrat", "modality": null, "already_gold_on_annotation": true, "labels_gold_exists": true, "gold_attributes": {}, "gen_attributes_on_annotation": {}, "annotated_attributes": {}, "gen_attributes": {}, "segmentation_error": false, "gen_attributes_on_full_annotation": {}, "full_gold_exists": false}}, "annotators": ["03"], "labels_gold": false, "full_gold": false}